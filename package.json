  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.jsx",
      "!src/constants/*.js",
      "!src/constants/*.jsx",
      "!src/**/*.stories.jsx",
      "!src/stories/**/*.{js,jsx}",
      "!src/test/**/js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "html",
      "json",
      "json-summary"
    ],
    "transformIgnorePatterns": [
      "../.*(node_modules)(?!.*(@ckeditor|ckeditor|ckeditor5-editor-classic|ckeditor5-react|ckeditor5-build|ckeditor5-core|ckeditor5-highlight|ckeditor5-utils|ckeditor5-ui|lodash-es).*).js$"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
